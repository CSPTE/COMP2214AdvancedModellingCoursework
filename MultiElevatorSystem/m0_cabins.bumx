machine m0_cabins
sees c0_cabins
variables cabins cabinFloor

invariants
@inv1: cabins ⊆ CABINS
@inv2: cabinFloor ∈ cabins ↣ floor

events
event INITIALISATION
begin
@act1: cabins ≔ ∅
@act2: cabinFloor ≔ ∅
end

event AddCabin
any c where
@grd1: c ∈ CABINS
@grd2: c ∉ cabins
@grd3: ∀ cab · cab ∈ cabins ∧ cabinFloor(cab) ≠ 1
then
@act1: cabins ≔ cabins ∪ {c}
@act2: cabinFloor(c) ≔ 1
end

end